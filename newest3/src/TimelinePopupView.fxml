<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<VBox fx:id="rootView" prefHeight="244.0" prefWidth="412.0" style="-fx-background-color: white;" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="TimelinePopupController">
  <children>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" style="-fx-background-color: white;" VBox.vgrow="ALWAYS">
         <children>
            <ButtonBar layoutY="204.0" prefHeight="40.0" prefWidth="406.0" style="-fx-background-color: white;">
              <buttons>
                <Button mnemonicParsing="false" onAction="#cancel" text="Cancel" />
                  <Button layoutX="331.0" layoutY="17.0" mnemonicParsing="false" onAction="#save" text="Save" />
              </buttons>
            </ButtonBar>
            <VBox layoutX="6.0" layoutY="4.0" prefHeight="201.0" prefWidth="399.0" style="-fx-background-color: white;">
               <children>
                  <Label text="Name" />
                  <TextField fx:id="timelineName" />
                  <Label text="Start Date" />
                  <DatePicker fx:id="startDate" />
                  <Label text="End Date" />
                  <DatePicker fx:id="endDate" />
               </children>
            </VBox>
         </children>
    </AnchorPane>
  </children>
</VBox>
